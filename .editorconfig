# http://editorconfig.org

root = true

[*]
chaset = utf-8
end_of_line = lf
indent_style = space
indent_size = 4
insert_final_newline = false
trim_trailing_whitespace = true

[*.html]
indent_size = 2

[*.scss]
indent_size = 2

[*.jade]
indent_size = 4

[*.md]
insert_final_newline = false
trim_trailing_whitespace = false
